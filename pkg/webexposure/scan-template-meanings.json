{
    "website-host-detection": {
        "label": "Web Server",
        "detection_template": ["Web App", "Web Server"],
        "finding_template": [
            "Web Server",
            "Live Domain",
            "{{if .ExtractedResults}}{{range $i, $v := .ExtractedResults}}{{if $i}}, {{end}}{{if eq $v \"Has Forms\"}}Authentication Forms{{else if eq $v \"Has SSO/Social Login\"}}SSO/Social Authentication{{else}}{{$v}}{{end}}{{end}}{{end}}"
        ]
    },
    "api-server-detection": {
        "label": "API Server Detection",
        "detection_template": ["API", "Serving JSON/XML"],
        "finding_template": [
            "Serving JSON/XML",
            "{{if .ExtractedResults}}{{range $i, $v := .ExtractedResults}}{{if $i}}, {{end}}{{$v}}{{end}}{{end}}"
        ]
    },
    "backend-framework-detection": {
        "label": "Backend Framework",
